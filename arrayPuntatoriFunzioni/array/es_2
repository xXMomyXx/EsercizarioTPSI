#include <stdio.h>

int main(int argc, char *argv[]){
    int l = 5;
    int arr [l];
    printf("Inserire 5 numeri\n");
    for(int i = 0; i < l; i++){
        scanf("%d", &arr[i]);
    }
    printf("Numeri inseriti al contrario:\n");
    for(int i = l - 1; i >= 0; i--){
        printf("%d\n", arr[i]);
    }
    return 0;
}

#include <stdio.h>

int main(int argc, char *argv[]){
    int l = 4;
    int arr [l];
    printf("Inserire 4 numeri\n");
    for(int i = 0; i < l; i++){
        scanf("%d", &arr[i]);
    }
    printf("Numeri inseriti:\n");
    for(int i = 0; i < l; i++){
        printf("%d\n", arr[i]);
    }
    return 0;
}
